
!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

!-- transparency ( any value under 255 ) --!
st.opacity:        255

st.borderpx: 13
!-- inner border -- ! 

st.font:          JetBrainsMono Nerd Font:style:medium :pixelsize=15 


! Base16 Gruvbox dark, hard

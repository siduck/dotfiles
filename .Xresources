!-- Xft settings -- !
Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault
Xft.dpi:   135

st.font:         JetBrainsMono Nerd Font:style:medium:pixelsize=20

! window border inner
st.borderpx: 20

